# Customize .bashrc for emulating a 5G core OSS
source  /home/student/.bashrc
alias 5gcup=      'sudo systemctl enable 5g-core.service ; sudo systemctl start 5g-core.service'
alias 5gcdown=    'sudo systemctl stop 5g-core.service ; sudo systemctl disable 5g-core.service'
alias 5gclog=     'sudo journalctl -fu 5g-core.service'
alias showamf=    'batcat /home/student/free5gc/config/amfcfg.yaml'
alias showausf=   'batcat /home/student/free5gc/config/ausfcfg.yaml'
alias shown3iwf=  'batcat /home/student/free5gc/config/n3iwfcfg.yaml'
alias shownrf=    'batcat /home/student/free5gc/config/nrfcfg.yaml'
alias shownssf=   'batcat /home/student/free5gc/config/nssfcfg.yaml'
alias showpcf=    'batcat /home/student/free5gc/config/pcfcfg.yaml'
alias showsmf=    'batcat /home/student/free5gc/config/smfcfg.yaml'
alias showudm=    'batcat /home/student/free5gc/config/udmcfg.yaml'
alias showupf=    'batcat /home/student/free5gc/NFs/upf/build/config/upfcfg.yaml'
alias showudr=    'batcat /home/student/free5gc/config/udrcfg.yaml'
alias showueroute='batcat /home/student/free5gc/config/uerouting.yaml'
alias showwebui=  'batcat /home/student/free5gc/config/webuicfg.yaml'
export PS1="\u@\h:\w_\[\e[33m\][\[\e[m\]\[\e[33m\]5gcore\[\e[m\]\[\e[33m\]]\[\e[m\]\\$ "
